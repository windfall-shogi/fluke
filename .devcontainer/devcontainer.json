{
    "name": "Python Development",
    "dockerComposeFile": "docker-compose.yml",
    "workspaceFolder": "/workspace",
    "service": "fluke",
    "customizations": {
      "vscode": {
        "settings": {
          "terminal.integrated.shell.linux": "/bin/bash"
        },
        "extensions": [
          "ms-python.python",
          "ms-python.vscode-pylance",
          "GitHub.copilot",
          "GitHub.copilot-chat",
          "ms-python.debugpy"
        ]
      }
    }
  }